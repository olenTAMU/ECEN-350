S_0x5587514d20 .scope module, "HalfAddTest" "HalfAddTest" 2 26;
 .timescale -9 -12;
v0x5587569a70_0 .var "A", 0 0;
v0x5587569b60_0 .var "B", 0 0;
v0x5587569c30_0 .net "Cout", 0 0, L_0x558756a050;  1 drivers
v0x5587569d30_0 .net "Sum", 0 0, L_0x5587569ef0;  1 drivers
v0x5587569e00_0 .var "passed", 7 0;
S_0x5587514eb0 .scope task, "allPassed" "allPassed" 2 43, 2 43 0, S_0x5587514d20;
 .timescale -9 -12;
v0x5587515090_0 .var "numTests", 7 0;
v0x5587568e60_0 .var "passed", 7 0;
TD_HalfAddTest.allPassed ;
    %load/vec4 v0x5587568e60_0;
    %load/vec4 v0x5587515090_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 47 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 48 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0x5587568f40 .scope module, "dut" "HalfAdd" 2 61, 3 1 0, S_0x5587514d20;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "Cout";
    .port_info 1 /OUTPUT 1 "Sum";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x5587569ef0 .functor XOR 1, v0x5587569a70_0, v0x5587569b60_0, C4<0>, C4<0>;
L_0x558756a050 .functor AND 1, v0x5587569a70_0, v0x5587569b60_0, C4<1>, C4<1>;
v0x5587569140_0 .net "A", 0 0, v0x5587569a70_0;  1 drivers
v0x5587569200_0 .net "B", 0 0, v0x5587569b60_0;  1 drivers
v0x55875692c0_0 .net "Cout", 0 0, L_0x558756a050;  alias, 1 drivers
v0x5587569390_0 .net "Sum", 0 0, L_0x5587569ef0;  alias, 1 drivers
S_0x5587569500 .scope task, "passTest" "passTest" 2 34, 2 34 0, S_0x5587514d20;
 .timescale -9 -12;
v0x55875696e0_0 .var "actualOut", 1 0;
v0x55875697c0_0 .var "expectedOut", 1 0;
v0x55875698a0_0 .var "passed", 7 0;
v0x5587569990_0 .var "testType", 120 0;
TD_HalfAddTest.passTest ;
    %load/vec4 v0x55875696e0_0;
    %load/vec4 v0x55875697c0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 39 "$display", "%s passed", v0x5587569990_0 {0 0 0};
    %load/vec4 v0x55875698a0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x55875698a0_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 40 "$display", "%s failed: %d should be %d", v0x5587569990_0, v0x55875696e0_0, v0x55875697c0_0 {0 0 0};
T_1.3 ;
    %end;
    .scope S_0x5587514d20;
T_2 ;
    %vpi_call 2 30 "$dumpfile", "HalfAddTest.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5587514d20 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5587514d20;
T_3 ;
    %pushi/vec4 0, 0, 1;
